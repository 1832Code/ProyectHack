# Copy this file to .env.local and replace with your real values for local development

# NextAuth configuration
NEXTAUTH_URL=https://proyecthacks.onrender.com
NEXTAUTH_SECRET=replace-with-a-secure-random-string

# Google OAuth 2.0 credentials
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Supabase - backend (server-only) service role key
# IMPORTANT: SUPABASE_SERVICE_ROLE_KEY must only be available on the server, do NOT expose this to the browser
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
# NextAuth / Google OAuth (required for real sign-in)
NEXTAUTH_URL=https://proyecthacks.onrender.com
NEXTAUTH_SECRET=replace-with-a-secure-random-string
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
